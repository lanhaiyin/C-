#include <iostream>
#include <stdio.h>
using namespace std;
int main()
{
 
char c;
    int arr[26]={0};
    while( ( c = getchar() ) != '\n' )
    {
        arr[c-'a']++;
    }
    for(int i=0;i<26;i++)
    {
        if(arr[i]>0)
        {
           printf("%c%d",i+'a',arr[i]);
        }
    }
    cout<<endl;
   
    return 0;
}
