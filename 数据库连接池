#include <iostream>
#include <string>
#include <vector>
using namespace std;
int main()
{
    int n;
    cin>>n;
    string s1,s2;
    int count=0;
   int flag=0;

    while(n--)
    {
        cin>>s1>>s2;
        if(s2=="connect"&&flag==0)
        {
           
             flag=1;
			 ++count;
        }
        else
        {
            flag=0;
        }
       
    }
    cout<<count<<endl;
    return 0;
}
