#include <iostream>
using namespace std;
 bool fun(int a,int b,int ans)
 {
     if(ans==24)
         return true;
     
     fun(a,b,a+b);
     fun (a-b);
     fun(a*b);
     fun(a/b);
 }
int main()
{
    int a,b,c,d;
    while(cin>>a>>b>>c>>d)
    {
      fun(a,b,c,d);
        
        
        
    }
    return 0;
}
